{
  "name": "n8n-nodes-singularity-data-lake",
  "version": "0.1.0",
  "description": "n8n node for SentinelOne Singularity Data Lake API",
  "keywords": [
    "n8n-community-node-package",
    "sentinelone",
    "singularity",
    "data-lake",
    "security",
    "siem"
  ],
  "license": "MIT",
  "homepage": "https://github.com/jmeltz/n8n-nodes-singularity-data-lake",
  "author": {
    "name": "jmeltz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmeltz/n8n-nodes-singularity-data-lake.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint 'nodes/**/*.ts' 'credentials/**/*.ts'",
    "lintfix": "eslint 'nodes/**/*.ts' 'credentials/**/*.ts' --fix",
    "prepublishOnly": "npm run build && npm run lint -q"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SentinelOneDataLakeApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/SentinelOneDataLake/SentinelOneDataLake.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.22.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
